import java.util.Scanner;
class PrimeNumbers
 {
   public static void main(String[]args)
   {
       Scanner sc = new Scanner(System.in);
       System.out.println("Enter the number of prime numbers:");
       int n = sc.nextInt();
       int sum = 0;
       for(int i =1;i<=n;++n)
       {
          if(i%2==0)
              sum+=i;
              }
        System.out.println("Sum of Prime Numbers: "+sum);
        }
    }
       
